{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test-clear": "npx jest --clearCache",
    "test": "jest",
    "test-watch-all": "test -- --coverage --watchAll=true",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/config/database/orm-migrations.config.ts",
    "migration-create": "npm run typeorm:cli -- migration:create -n created",
    "migration-generate": "npm run typeorm:cli -- migration:generate -n generated",
    "migrations-run": "npm run typeorm:cli -- migration:run",
    "migrations-revert": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.4",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.0.8",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": "[.]spec.ts$",
    "testPathIgnorePatterns": [],
    "coveragePathIgnorePatterns": [
      "<rootDir>/main.ts",
      "<rootDir>/config/app/app-configuration.ts",
      "<rootDir>/config/app/app-config.module.ts",
      "<rootDir>/config/database/database.config.ts",
      "<rootDir>/config/database/orm-migrations.config.ts",
      "<rootDir>/config/swagger/swagger-config.module.ts",
      "<rootDir>/config/swagger/swagger-configuration.ts",
      "<rootDir>/filters/fallback.filter.ts",
      "<rootDir>/filters/http.filter.ts",
      "<rootDir>/filters/validation.exception.ts",
      "<rootDir>/filters/validation.filter.ts",
      "<rootDir>/helpers/typeorm-logger.class.ts",
      "<rootDir>/interceptors/http-response.interceptor.ts",
      "<rootDir>/migrations/*.*",
      "<rootDir>/schema/app/app.module.ts",
      "<rootDir>/schema/common/models/classes/check-out-object.class.ts",
      "<rootDir>/schema/common/models/classes/check-out.class.ts",
      "<rootDir>/schema/common/models/classes/response-header.class.ts",
      "<rootDir>/schema/common/models/classes/response-seo.class.ts",
      "<rootDir>/schema/common/models/classes/response.class.ts",
      "<rootDir>/schema/common/models/dtos/pagination-details.dto.ts",
      "<rootDir>/schema/common/models/dtos/pagination-filter.dto.ts",
      "<rootDir>/schema/common/models/dtos/pagination-rq.dto.ts",
      "<rootDir>/schema/common/models/dtos/pagination-rs.dto.ts",
      "<rootDir>/schema/common/models/dtos/pagination-sort.dto.ts",
      "<rootDir>/schema/common/models/dtos/search.dto.ts",
      "<rootDir>/schema/common/models/enums/filter-type.enum.ts",
      "<rootDir>/schema/common/models/enums/pagination-page-size.enum.ts",
      "<rootDir>/schema/common/models/enums/sort-type.enum.ts",
      "<rootDir>/schema/post/post.module.ts",
      "<rootDir>/schema/post/entities/post.entity.ts",
      "<rootDir>/schema/post/models/dtos/create-post.dto.ts",
      "<rootDir>/schema/post/models/dtos/post-search.dto.ts",
      "<rootDir>/schema/post/models/dtos/post.dto.ts",
      "<rootDir>/schema/post/models/dtos/update-post.dto.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
